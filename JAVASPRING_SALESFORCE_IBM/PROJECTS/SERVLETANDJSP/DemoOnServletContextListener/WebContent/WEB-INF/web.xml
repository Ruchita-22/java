<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>DemoOnServletContextListener</display-name>
  
  <listener>
    <listener-class>com.ibm.listeners.ExampleOnServeltContextListener</listener-class>
  </listener>
 
<servlet>
    <description></description>
    <display-name>HelloServelt</display-name>
    <servlet-name>HelloServelt</servlet-name>
    <servlet-class>com.ibm.services.HelloServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServelt</servlet-name>
    <url-pattern>/hs</url-pattern>
  </servlet-mapping>
</web-app>