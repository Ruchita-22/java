<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <context-param>
    <param-name>drv</param-name>
    <param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </context-param>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:oracle:thin:@127.0.0.1:1521:xe</param-value>
  </context-param>
  <context-param>
    <param-name>username</param-name>
    <param-value>scott</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>ExampleOnServletContext</display-name>
    <servlet-name>ExampleOnServletContext</servlet-name>
    <servlet-class>com.ibm.ExampleOnServletContext</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExampleOnServletContext</servlet-name>
    <url-pattern>/esctx</url-pattern>
  </servlet-mapping>
</web-app>