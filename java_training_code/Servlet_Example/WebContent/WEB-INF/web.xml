<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlet_Example</display-name>
  <context-param>
    <param-name>drv</param-name>
    <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:mysql://localhost:3306/sonoo</param-value>
  </context-param>
  <context-param>
    <param-name>username</param-name>
    <param-value>root</param-value>
  </context-param>
  <servlet>
    <servlet-name>ContextExample</servlet-name>
    <servlet-class>com.ibm.contextExample.ContextExample</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContextExample</servlet-name>
    <url-pattern>/ce</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>RequestDispatcherExample</servlet-name>
    <servlet-class>com.ibm.redirectExample.RequestDispatcherExample</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestDispatcherExample</servlet-name>
    <url-pattern>/rd</url-pattern>
  </servlet-mapping>
</web-app>